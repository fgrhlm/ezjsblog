{
  "name": "ezjsblog",
  "version": "1.0.0",
  "description": "EZ backend for your blog.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "serve": "EZJSBLOG_MODE=prod node --experimental-modules src/index.js",
    "dev": "nodemon src/index.js",
